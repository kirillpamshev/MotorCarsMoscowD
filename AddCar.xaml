        <ComboBox x:Name="seat_count_text" HorizontalAlignment="Left" Margin="346,130,0,0" VerticalAlignment="Top" Width="31" Visibility="Hidden" />
        <ComboBox x:Name="add_year_text" HorizontalAlignment="Left" Margin="346,167,0,0" VerticalAlignment="Top" Width="194" Height="28" Visibility="Hidden"/>
